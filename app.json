{
  "expo": {
    "name": "iSleep",
    "description": "An app to help you rest.",
    "slug": "isleep",
    "privacy": "public",
    "sdkVersion": "21.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon-eye.png",
    "notification": {
      "icon": "./assets/icons/notification-icon.png",
      "color": "#000000"
    },
    "loading": {
      "icon": "./assets/icons/app-icon-eye.png",
      "hideExponentText": true
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "nonPersistent": true
     }
  }
}
